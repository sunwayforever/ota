<%= form_for @deltum, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :a_version_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.select :a_version_id, Version.all.collect {|v| [v.version,v.id]} %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :b_version_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.select :b_version_id, Version.all.collect {|v| [v.version,v.id]} %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :path, :class => 'control-label' %>
    <div class="controls">
      <%= f.file_field :deltum_file, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :product_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.select :product_id, Product.all.collect {|p| [p.vender,p.id]} %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                delta_path, :class => 'btn' %>
  </div>
<% end %>
